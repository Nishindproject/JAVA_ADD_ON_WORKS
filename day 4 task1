class Over {
    
    public void sum(int a, int b) {
        int sum = a + b;
        System.out.println("Sum of the integers: " + sum);
    }

    public void sum(double a, double b) {
        double dou = a + b;
        System.out.println("Sum of the doubles: " + dou);
    }
}

public class Main {
    public static void main(String[] args) {
        Over o = new Over();
        o.sum(50, 70);         
        o.sum(50.00, 70.12);   
    }
}
